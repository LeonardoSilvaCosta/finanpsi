generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Lead {
  id            String   @id @default(cuid())
  name          String
  email         String   @unique
  profession    String?
  challenge     String?
  groupAccepted Boolean  @default(false)
  diagnosis     String?
  createdAt     DateTime @default(now())
}

